    <RmGLShader NAME="Vertex Program" API="OpenGL" PIXEL_SHADER="FALSE"><RmShaderCode TYPE="TEXT"><![CDATA[
varying vec3 normalVec;
varying vec3 viewVec;

void main(void)
{
   gl_Position = ftransform();
   gl_TexCoord[0] = gl_MultiTexCoord0;
   
   normalVec = gl_NormalMatrix * gl_Normal;
   viewVec = -vec3(gl_ModelViewMatrix * gl_Vertex);
}]]></RmShaderCode></RmGLShader>
    <RmGLShader NAME="Fragment Program" API="OpenGL" PIXEL_SHADER="TRUE"><RmShaderCode TYPE="TEXT"><![CDATA[
uniform sampler2D base;
uniform sampler2D bump;
uniform float Ka;
uniform float Kd;
uniform float Ks;

varying vec3 normalVec;
varying vec3 viewVec;

const vec3 lightVec = vec3(0.4, 0.4, 0.8);

void main(void)
{
   vec4 baseColor = texture2D(base, gl_TexCoord[0].st);
   vec3 bumpPert = texture2D(bump, gl_TexCoord[0].st).rgb;

   vec3 nLight = normalize(lightVec);
   vec3 nView = normalize(viewVec);
   vec3 nNormal = normalize(normalVec + bumpPert);

   float diffuse = clamp(dot(nLight, nNormal), 0.0, 1.0);
   float specular = pow(clamp(dot(reflect(-normalize(nView), nNormal), nLight),0.0, 1.0), 16.0);

   gl_FragColor = Ka * baseColor +
                  Kd * diffuse * baseColor + Ks * specular;
}]]></RmShaderCode></RmGLShader>
