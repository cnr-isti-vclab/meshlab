      \fi%
    }{%
      %what is to be put into movie link annot depends on the following
      \ifx\@MXV@label\@empty\else%
        \@MXV@labeltoaux{@MXV@\@MXV@label.externalviewer}{false}%
        \@MXV@newlabel{@MXV@\@MXV@label.externalviewer}{false}%
      \fi%
      %
