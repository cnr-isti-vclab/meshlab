      `start'/`endat' or `start'/`duration'
    }{}%
  \fi\fi%
  %
  %option list
  \edef\@MXV@movieoptions{#1\@MXV@autostart\@MXV@loop\@MXV@showcontrols}%
  %
  %determine playback section
  \ifx\@MXV@starttime\@empty\else%
    \edef\@MXV@movieoptions{\@MXV@movieoptions, %
