      \ifthenelse{\boolean{@MXV@iiid}}{%
        % #### 3D ####
        \if@MXV@externalviewer\else%
          \ifthenelse{\equal{\@MXV@iiidpalindrome}{/Subtype/Oscillating}}{%
            \global\multiply\@MXV@iiidrepeat by 2%
